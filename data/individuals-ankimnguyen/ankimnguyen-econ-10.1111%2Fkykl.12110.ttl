                             citec:isPresent false ; # true/false

                           ] ;
          ] ;
      ca:isTargetOf
          [ rdf:type ca:CodeApplication ;
            ca:hasCoder "ankimnguyen" ;
            ca:appliesCode [ rdf:type citec:url ;
                             citec:isPresent false ; # true/false

                           ] ;
          ] ;
      ca:isTargetOf
          [ rdf:type ca:CodeApplication ;
            ca:hasCoder "ankimnguyen" ;
            ca:appliesCode [ rdf:type citec:creator ;
                             citec:isPresent true ; # true/false
                             rdfs:label """Roodman""" ; # use triple quotes
                           ] ;
          ] ;
      citec:has_reference doi-cited:10.1111%2Fkykl.12110_Roodman-2009 ; # name reference like doi-cited:a2004-40-NAT_GENET_Author-YYYY, no quotes
  .
