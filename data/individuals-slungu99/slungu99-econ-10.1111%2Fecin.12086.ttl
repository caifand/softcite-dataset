.
doi:10.1111%2Fecin.12086_SL01 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """The computers were refurbished Pentium III450 MHz machines with 256 MB RAM, 10 GB hard drives,17" monitors, modems, ethernet cards, C D drives, and Win-dows 2000 Pro Open Ofî€ƒce (with Word, Excel, and Pow-erPoint).""" ; # use triple quotes

    citec:on_pdf_page 4  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "slungu99" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label """hardware"""; # put one in triple quotes
                           ca:certainty 9 ; # integer
                           ca:memo """mention of a computer usage""" ; # use triple quotes
                         ] ;
        ] ;
.
