doi:10.1111%2Fkykl.12110 rdf:type bioj:article ;
    rdf:type bioj:econ_article ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ankimnguyen" ;
          ca:appliesCode [ rdf:type citec:codable ;
                           citec:isPresent true; # true if can code
                         ] ;
        ] ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ankimnguyen" ;
          ca:appliesCode [ rdf:type citec:standard_type ;
                           citec:isPresent true ; # true if standard article
                         ] ;
        ] ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ankimnguyen" ;
          ca:appliesCode [ rdf:type citec:coded_no_in_text_mentions ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;


    citec:has_in_text_mention doi:10.1111%2Fkykl.12110_AN1 ;
    # create name for in_text_mention like
    # doi:10.1111%2Fkykl.12110_JH01

    # citations like:
    # doi-cited:10.1111%2Fkykl.12110_AuthorYear
.
doi:10.1111%2Fkykl.12110_AN1 rdf:type citec:in_text_mention ; # use in text mention name
      citec:full_quote """ To keep the number of instruments at a minimum, we use the collapse option in STATA in all regressions (Roodman, 2009). """ ; # use triple quotes

      citec:on_pdf_page 15  ; # integer

      citec:spans_pages false ; # true/false

      ca:isTargetOf
          [ rdf:type ca:CodeApplication ;
            ca:hasCoder "ankimnguyen" ;
            ca:appliesCode [ rdf:type citec:mention_type ;
